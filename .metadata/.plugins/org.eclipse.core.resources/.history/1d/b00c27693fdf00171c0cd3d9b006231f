package fr.dessin;

public class Point3D extends Point2D {
	
	private int z;
	
	public Point3D() {
		this(0, 0, 0);
	}

	public Point3D(int vX, int vY, int vZ) {
		super(vX, vY);
		setZ(vZ);
	}

	public int getZ() {
		return z;
	}

	public void setZ(int z) {
		this.z = z;
	}

	@Override
	public void afficher() {
		System.out.println("[ " + super.getX() + ", " + super.getY() + ", " + this.getZ() + "]");
	}

	
	

}
